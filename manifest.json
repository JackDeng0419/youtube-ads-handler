{
    "manifest_version": 3,
    "name": "Ad Speedup and Skipper",
    "version": "1.0",
    "description": "Speeds up ads and automatically skips them when possible",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
  }